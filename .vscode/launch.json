    "version": "0.2.0",
    "configurations": [
        {
            "name": "Evaluate",
            "type": "debugpy",
            "request": "launch",
            "program": "evaluate.py",
            "console": "integratedTerminal",
            "env": {
                "PYDEVD_DISABLE_FILE_VALIDATION": "1"
            }
        },
        {
            "name": "Generate",
            "type": "debugpy",
            "request": "launch",
            "program": "generate.py",
            "console": "integratedTerminal",
            "args": ["--restart"],
            "env": {
                "PYDEVD_DISABLE_FILE_VALIDATION": "1"
            }
        },
    ]
}